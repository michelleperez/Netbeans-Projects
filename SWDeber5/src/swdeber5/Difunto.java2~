        this.fecha_afiliacion = val;
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,id=DCE.2F172D95-F460-7E43-8365-FD331DD8C0D8]
    // </editor-fold> 
    public void Mostrar () {
        utl.PrintDate(fecha_afiliacion,"dd/MM/yyyy");
        utl.Println("Causa de muerte: "+causa+"\tEstado: "+estado);
      
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,id=DCE.109929B9-6B5F-734A-19E6-FFF67D93445E]
    // </editor-fold> 
    public int CalcularEdad () {
             
        return 0;
    }

}

