    public void Mostrar () {
        utl.Println("Nombre: "+nombre+"\tCÃ©dula: "+cedula);
        utl.Println("Fecha de nacimiento: "+utl.PrintDate(fecha_nacimiento, "dd/MM/yyyy"));
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,id=DCE.C9D73518-7CD6-9EDF-E968-F5768EEB4544]
    // </editor-fold> 
