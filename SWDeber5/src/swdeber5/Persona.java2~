    private String cedula;

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,id=DCE.CA549F8B-0AEB-C891-440C-98E0BD190F63]
    // </editor-fold> 
    private String nombre;

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,id=DCE.259BA63E-7F74-4175-3C70-5B77C98E3D22]
    // </editor-fold> 
    private Calendar fecha_nacimiento;

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,id=DCE.E933E1FA-2893-FB21-DA8C-0D13CB7591A7]
    // </editor-fold> 
    public Persona () {
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,regenBody=yes,id=DCE.790E6948-A63D-0914-00B3-406B48D8EEA3]
