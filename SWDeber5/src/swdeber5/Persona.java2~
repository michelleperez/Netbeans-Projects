    public String getCedula () {
        return cedula;
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,regenBody=yes,id=DCE.49894400-E671-D5FA-66B9-6CC4AB130353]
    // </editor-fold> 
    public void setCedula (String val) {
        this.cedula = val;
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,regenBody=yes,id=DCE.C40C1DA9-7EFA-E76B-19E0-C8120DF39E8A]
