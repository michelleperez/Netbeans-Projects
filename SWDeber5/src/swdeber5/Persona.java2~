    public Calendar getFecha_nacimiento () {
        return fecha_nacimiento;
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,regenBody=yes,id=DCE.FE73EBA1-2317-3A76-8CC5-74362E6CE2FC]
